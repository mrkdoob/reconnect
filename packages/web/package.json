{
  "private": true,
  "name": "@reconnect/admin",
  "version": "1.0.0",
  "scripts": {
    "start": "BROWSER=none TSC_COMPILE_ON_ERROR=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "*.{json,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --ext .jsx,.js,.ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.16",
    "@apollo/react-hooks": "3.1.3",
    "@babel/helper-call-delegate": "^7.8.7",
    "@chakra-ui/core": "0.5.2",
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@noquarter/hooks": "1.1.13",
    "@noquarter/utils": "1.0.8",
    "@reach/router": "1.2.1",
    "@sentry/node": "^5.14.2",
    "bullmq": "^1.6.6",
    "dayjs": "1.8.18",
    "emotion-theming": "10.0.27",
    "express-jwt": "^5.3.1",
    "graphql": "^14.5.8",
    "interweave": "^12.1.1",
    "limax": "^2.0.0",
    "react": "^16.12.0",
    "react-beautiful-dnd": "12.2.0",
    "react-dom": "16.12.0",
    "react-dropzone": "10.2.1",
    "react-hook-form": "4.4.1",
    "react-scripts": "3.4.0",
    "routing-controllers": "^0.8.0",
    "styled-components": "^4.4.1",
    "styled-icons": "^9.1.0",
    "typescript": "^3.8.3",
    "yup": "0.28.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.10.0",
    "@graphql-codegen/typescript": "^1.10.0",
    "@graphql-codegen/typescript-operations": "^1.10.0",
    "@graphql-codegen/typescript-react-apollo": "^1.10.0",
    "@types/jest": "24.0.25",
    "@types/node": "12.12.22",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.17",
    "@types/react-beautiful-dnd": "11.0.4",
    "@types/react-dom": "16.9.4",
    "@types/yup": "0.26.27",
    "eslint-config-react-app": "5.1.0",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "graphql-tag.macro": "2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
